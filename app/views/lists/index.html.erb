<div class="container mt-3">
  <div class="d-flex justify-content-between align-items-center">
    <h1>Lists</h1>
    <%= link_to new_list_path, class: 'text-dark text-decoration-none' do %>
      <i class="fa-solid fa-plus"></i>
    <% end %>
  </div>
  <div class="cards">
    <% @lists.each do |list| %>
      <%= render 'lists/card', list: list %>
    <% end %>
  </div>
</div>
